<div>
    <ul id="slider">
        @foreach (var image in Model)
        {
            <li><img src="~/Content/Images/@Url.Content(image)" /></li>
        }
    </ul>
</div>
<script>
    $(document).ready(function () {
        $('#slider').bxSlider();
        $('.bx-wrapper').width('70%');
    });
</script>

